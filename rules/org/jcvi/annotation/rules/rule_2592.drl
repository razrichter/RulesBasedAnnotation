package org.jcvi.annotation.rules
import org.jcvi.annotation.facts.*;

// rule 2592 DEFAULT_METHOD[GB|CAL84265.1]
rule "2592|flagellar glycosylation island protein, CBO2704 family"
no-loop true
when
	blast_hit1 : BlastHit( program=='blastp', 
				hitId=='GB|CAL84265.1', 
				bitScore >= 160,
				hitPercentLength >= 90,
				queryPercentLength >= 90,  
				percentIdentity >= 80,
				hitQueryLengthRatio <= 5.1)
	gene1 : Feature( featureId == blast_hit1.queryId )


then
	Annotation a = new Annotation("2592");
	a.setCommonName("flagellar glycosylation island protein, CBO2704 family");
	a.setEcNumbers(" ");
	a.setGeneSymbol(" ");
	a.setGoIds("GO:0003674,GO:0005575,GO:0008150");
	a.setRoleIds("91");
	a.setConfidence(97);
	a.setSpecificity(SpecificityType.INIT_EQUIV);
	gene1.addAssertedAnnotation(a);
	update(gene1);
end